{
  "expo": {
    "name": "alabamas",
    "slug": "alabamas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "fonts": [
      {
        "asset": "assets/fonts/Poppins-Regular.ttf",
        "family": "Poppins"
      },
      {
        "asset": "assets/fonts/Poppins-Bold.ttf",
        "family": "Poppins-Bold"
      },
      {
        "asset": "assets/fonts/Poppins-Medium.ttf",
        "family": "Poppins-Medium"
      },
      {
        "asset": "assets/fonts/CrimsonPro-Medium.ttf",
        "family": "CrimsonPro-Medium"
      },
      {
        "asset": "assets/fonts/CrimsonPro-Regular.ttf",
        "family": "CrimsonPro-Regular"
      }
    ],
    "ios": {
      "supportsTablet": true,
      "statusBar": {
        "backgroundColor": "#2196F3",
        "style": "light"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "a03d9f0c-87b7-4913-a1d3-68029f02fff2"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a03d9f0c-87b7-4913-a1d3-68029f02fff2"
    }
  }
}
